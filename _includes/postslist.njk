<article class="{{post.data.status}}">
<h4><a href="{{post.url}}">{{ post.data.title }}</a></h4>
<img src="{{post.data.cover}}" alt="">
<p>{{ post.data.excerpt | safe }}</p>
<span class="post-meta">
{% for tag in post.data.tags %}
{% if tag != "posts" %}
{{ tag }}</span>
{% endif %}
{% endfor %}
</span>
</article>
{%- endfor %}